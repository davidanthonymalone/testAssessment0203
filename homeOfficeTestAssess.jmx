              <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="UpdateRetryCount">
                <stringProp name="script">
if (!prev.isSuccessful()) {

    int c = Integer.parseInt(vars.get(&quot;retryCount&quot;));
    c++;
    vars.put(&quot;retryCount&quot;, Integer.toString(c));
    // If we&apos;ve reached 3 attempts, stop the thread
    if (c &gt;= 3) {
        log.info(&quot;Auth request failed after 3 attempts, stopping thread&quot;);
        ctx.getThread().stop();
    }
} else {
    // Mark success to break the while loop on next evaluation
    vars.put(&quot;successFlag&quot;, &quot;true&quot;);
    // Set retryCount to 3 to satisfy the condition as a fallback
    vars.put(&quot;retryCount&quot;, &quot;3&quot;);
}
</stringProp>
                <stringProp name="scriptLanguage">groovy</stringProp>
